# Executable load path
export PATH="./.bin:./bin:$HOME/.rbenv/shims:/usr/local/bin:/usr/local/sbin:$PATH"

# Man search path
export MANPATH="/usr/local/man:$MANPATH"

# Add Postgres command line tools to path
export PATH="$PATH:/Applications/Postgres.app/Contents/Versions/9.3/bin"

# Heroku Toolbelt path
export PATH="$PATH:/usr/local/bin/heroku"
